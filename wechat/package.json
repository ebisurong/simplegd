{
  "name": "workorder-management-system",
  "version": "1.0.0",
  "description": "工单管理系统 - 全栈项目",
  "main": "start.js",
  "scripts": {
    "start": "node start.js",
    "dev": "node start.js",
    "install-deps": "cd frontend && npm install && cd ../backend && npm install",
    "build": "cd frontend && npm run build",
    "init-db": "cd backend && npm run init-db",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "clean": "rm -rf frontend/node_modules backend/node_modules frontend/dist backend/database.sqlite"
  },
  "keywords": [
    "工单管理",
    "vue3",
    "express",
    "sqlite",
    "element-plus",
    "全栈开发"
  ],
  "author": "工单管理系统开发团队",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "dependencies": {
    "axios": "^1.12.2",
    "form-data": "^4.0.4"
  }
}
